{
    "name": "keboola/docker-lgr-bundle",
    "type": "symfony-bundle",
    "description": "LuckyGuess R Backend for running in Keboola Docker",
    "keywords": [
        "keboola",
        "luckyguess",
        "r",
        "docker"
    ],
    "authors": [
        {
            "name": "OndÅ™ej Popelka",
            "email": "ondrej.popelka@keboola.com"
        }
    ],
    "repositories": [],
    "require": {
        "php": ">=5.4.0",
        "keboola/syrup": "~2.1.0",
        "keboola/provisioning-client": "0.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {"Keboola\\DockerLGRBundle": ""}
    },
    "target-dir": "Keboola/DockerLGRBundle",
    "extra": {
        "syrup-app-name": "docker-lgr-bundle",
        "symfony-app-dir": "vendor/keboola/syrup/app",
        "symfony-web-dir": "vendor/keboola/syrup/web",
        "incenteev-parameters": {
            "keep-outdated": true,
            "file": "vendor/keboola/syrup/app/config/parameters.yml",
            "dist-file": "parameters.yml"
        }
    }
}